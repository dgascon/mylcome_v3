{
  "name": "mylcome_v3",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "ts-node-esm src/main.ts",
    "start": "node build/main.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node-esm' src/main.ts",
    "watch": "nodemon --exec ts-node-esm src/main.ts",
    "prisma:generate": "yarn prisma generate",
    "prisma:push": "yarn prisma db push"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^3.0.0",
    "@prisma/client": "^4.2.1",
    "discord.js": "^14.0.2",
    "discordx": "^10.0.0",
    "dotenv": "^16.0.1",
    "fs": "^0.0.1-security",
    "prisma": "^4.2.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}